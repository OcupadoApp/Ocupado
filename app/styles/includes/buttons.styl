.back
  margin-top 50px
  border 1px solid #fff
  border-radius 3px
  background transparent
  position absolute
  color #fff

.book-now-btn
  position absolute
  width 50px;
  height 50px
  left 50%
  margin-left -25px
  top 50%
  margin-top -15px
  border 1px solid #57616c
  border-radius 50%

  &:before
    content: '+'
    font-family 'bebas'
    display block
    font-size 60px
    width 100%
    text-align center
    line-height 46px
